            var catdefault = "altri";
            var catref = document.referrer;
            // var catref = 'saronnonews.it/category/economia/';
            console.edilog('catref',catref);


            let site_domain = "legnanonews.com";
            // site_domain = site_domain.replace('.','\\.');

            const regexp_cat = new RegExp(site_domain+"/category/([a-z0-9-]*)/");
            console.edilog('regexp',regexp_cat);
            var matches = catref.match(regexp_cat);
            console.edilog('matches',matches);

            let usecat;
            if (matches != null) usecat= matches[1];
            else usecat = catdefault;


            var permalink = "https://www.legnanonews.com/altri/2020/12/23/ambiente-e-inquinamento-la-natura-puo-aiutare/963689/";
            const regexp_link = new RegExp(site_domain);
            var pagparam = permalink.replace(regexp_link, site_domain+"/"+usecat);
            console.edilog('pagparam',pagparam);

        </script>

